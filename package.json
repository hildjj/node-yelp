{
  "name": "node-yelp",
  "version": "0.0.1",
  "description": "Simple library interfacing with Yelp API v2.0",
  "main": "./lib",
  "dependencies": {
    "bunyan": "~0.22.0",
    "config": "~0.4.32"
  },
  "devDependencies": {
    "mocha": "~1.14.0",
    "chai": "~1.8.1",
    "jshint": "~2.3.0",
    "jshint-stylish": "~0.1.3",
    "mocha-lcov-reporter": "0.0.1",
    "coveralls": "~2.8.0",
    "blanket": "~1.1.6"
  },
  "scripts": {
    "start": "node . | ./node_modules/.bin/bunyan",
    "nodemon": "nodemon --debug . | ./node_modules/.bin/bunyan",
    "test": "npm run lint && npm run mocha",
    "mocha": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec test/",
    "coverage": "NODE_ENV=test ./node_modules/.bin/mocha -r blanket  -R html-cov test/ > coverage.html",
    "coveralls": "NODE_ENV=test ./node_modules/.bin/mocha -r blanket  -R mocha-lcov-reporter test/ | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "./node_modules/.bin/jshint --reporter ./node_modules/jshint-stylish/stylish.js ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Woorank/node-yelp"
  },
  "keywords": [
    "yelp"
  ],
  "author": "WooRank SPRL <dev@woorank.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Woorank/node-yelp/issues"
  },
  "engines": {
    "node": ">=0.10"
  },
  "config": {
    "blanket": {
      "pattern": "lib/",
      "data-cover-never": "node_modules/",
      "data-cover-reporter-options": {
        "shortnames": false
      }
    }
  }
}
